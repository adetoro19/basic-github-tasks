							"listen": "test",
							"script": {
								"exec": [
									" \r",
									"pm.test(\"200 is expected as status code\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"pm.test(\"Status code name has string OK\", function () {\r",
									"    pm.response.to.have.status(\"OK\");\r",
									"});\r",
									"\r",
									"var jsonData = pm.response.json();\r",
									"console.log(jsonData)\r",
									"console.log(jsonData.apiVersion)\r",
									"console.log(jsonData.title)\r",
									"console.log(jsonData.paymentCurrency)\r",
									"console.log(jsonData.links[3].rel)\r",
									"\r",
									"pm.test(\"Test for the API Version is 7.2.3.0 \", function () {\r",
									"    pm.expect(jsonData.apiVersion).to.eql(\"7.2.3.0\");\r",
									"});\r",
									" pm.test(\"Confirm that title is Fusio \", function () {\r",
									"    pm.expect(jsonData.title).to.include(\"Fusio\");});\r",
									"\r",
									"pm.test(\"Confirm that entry 3 is route\", function () {\r",
									"    pm.expect(jsonData.links[3].rel).to.eql(\"route\");});\r",
									"\r",
									"pm.test(\"Payment currebcy is EUR\", function () {\r",
									"    pm.expect(jsonData.paymentCurrency).to.contain(\"EUR\");\r",
									"});\r",
									"pm.test(\"Confirm that entry 3 has the URL https://demo.fusio-project.org/system/route\", function () {\r",
									"    pm.expect(jsonData.links[3].href).to.eql(\"https://demo.fusio-project.org/system/route\");});\r",
